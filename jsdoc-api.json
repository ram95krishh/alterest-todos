{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "source": {
        "include": ["imports/api"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_",
        "exclude": []
    },
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "destination": "docs/api",
        "recurse": true,
        "readme": "README.md"
    }
}